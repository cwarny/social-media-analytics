<div class="col-md-8">
	<div class="well row tweetbox">
		<div class="col-md-1">
			<img {{bindAttr src=user.profile_image_url}} alt="Profile picture">
		</div>
		<div class="col-md-11">
			<blockquote>
				<p>{{text}}</p>
				<small>{{user.name}} (@{{user.screen_name}})</small>
			</blockquote>
		</div>
		<button type="button" class="btn btn-primary pull-right">See conversation</button>
		{{bar-chart data=clicks}}
	</div>
</div>